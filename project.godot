[application]

config/name="toy roguelike"
run/main_scene="res://scenes/IntroMap.tscn"
run/delta_sync_after_draw=true
boot_splash/image="res://art/black.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"

[autoload]

Scheduler="*res://singletons/Scheduler.gd"
HUD="*res://singletons/HUD.tscn"
EmitterFactory="*res://singletons/EmitterFactory.gd"
Manager="*res://singletons/Manager.tscn"
SaveData="*res://singletons/SaveData.gd"

[display]

window/size/width=1280
window/size/height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

[gui]

common/snap_controls_to_pixels=false

